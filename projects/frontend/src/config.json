{
  "urls": {
    "logout": "/logout",
    "userInfo": "/api/v1/account",
    "keycloak": {
      "userSearch": "/api/keycloak/users",
      "usersForRole": "/api/keycloak/roles/{roleName}/users?max=1000",
      "roleMappings": "/api/keycloak/users/{userId}/role-mappings/realm",
      "role": "/api/keycloak/roles/{roleName}"
    },
    "version": "/config/version.json",
    "workspaces": "/api/v1/workspaces",
    "clusterInfo": "/api/v1/cluster"
  },
  "elasticsearch": {
    "host": "/api/v1/search",
    "log": "error"
  },
  "searchIndex": "hyperspace",
  "externalConfigurationFiles": [
    "/config/config.json"
  ],
  "roles": {
    "organisationAdmin": "",
    "workspaceRoles": [
      "user",
      "coordinator",
      "datasteward",
      "sparqluser"
    ],
    "prefixes": {
      "user": "user",
      "coordinator": "coordinator"
    }
  }
}
